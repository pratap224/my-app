<h1>Users Details</h1>
<a href="<%=users_new_path %>">New User</a>
<table>
	<tr> 
	<th>First Name</th>
	<th>Last Name</th>
	<th>Contact</th>	
	</tr>
	<tr>
	<% @users.each do |u| %>
	<td><%=u.first_name %></td>
	<td><%=u.last_name %></td>
	<td><%=u.contact %></td>
	<td>
		<a href="<%=users_show_path (u.id) %>">show</a>
		<a href="<%=users_edit_path (u.id) %>">edit</a>
		<a href="<%=users_destroy_path (u.id) %>">destroy</a>
	</td>	
	</tr>
	<%end %>

</table>

